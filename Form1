        private void button2_Click(object sender, EventArgs e)
        {
            Memorisation memo = new Memorisation(20, 10);
            pictureBox1.ImageLocation = memo.ListeTest[0].Image;
            pictureBox2.ImageLocation = memo.ListeTest[1].Image;
            pictureBox3.ImageLocation = memo.ListeTest[2].Image;
            pictureBox4.ImageLocation = memo.ListeTest[3].Image;
            pictureBox5.ImageLocation = memo.ListeTest[4].Image;
            pictureBox6.ImageLocation = memo.ListeTest[5].Image;
            pictureBox7.ImageLocation = memo.ListeTest[6].Image;
            pictureBox8.ImageLocation = memo.ListeTest[7].Image;
            pictureBox9.ImageLocation = memo.ListeTest[8].Image;
            pictureBox10.ImageLocation = memo.ListeTest[9].Image;
            textBox1.Text = " " + memo.ListeTest[0].NumeroJeu;
            textBox2.Text = " " + memo.ListeTest[1].NumeroJeu;
            textBox3.Text = " " + memo.ListeTest[2].NumeroJeu;
            textBox4.Text = " " + memo.ListeTest[3].NumeroJeu;
            textBox5.Text = " " + memo.ListeTest[4].NumeroJeu;
            textBox6.Text = " " + memo.ListeTest[5].NumeroJeu;
            textBox7.Text = " " + memo.ListeTest[6].NumeroJeu;
            textBox8.Text = " " + memo.ListeTest[7].NumeroJeu;
            textBox9.Text = " " + memo.ListeTest[8].NumeroJeu;
            textBox10.Text = " " + memo.ListeTest[9].NumeroJeu;

            textBox1.Visible = true;
            textBox2.Visible = true;
            textBox3.Visible = true;
            textBox4.Visible = true;
            textBox5.Visible = true;
            textBox6.Visible = true;
            textBox7.Visible = true;
            textBox8.Visible = true;
            textBox9.Visible = true;
            textBox10.Visible = true;

            pictureBox1.Visible = true;
            pictureBox2.Visible = true;
            pictureBox3.Visible = true;
            pictureBox4.Visible = true;
            pictureBox5.Visible = true;
            pictureBox6.Visible = true;
            pictureBox7.Visible = true;
            pictureBox8.Visible = true;
            pictureBox9.Visible = true;
            pictureBox10.Visible = true;

            TestMemo.Visible = false;
            Retour.Visible = true;
        }

